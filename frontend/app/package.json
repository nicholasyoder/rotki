{
  "name": "rotki",
  "version": "1.41.3",
  "description": "A portfolio tracking, asset analytics and tax reporting application specializing in Cryptoassets that protects your privacy",
  "type": "module",
  "license": "AGPL-3.0",
  "homepage": "https://rotki.com",
  "repository": "https://github.com/rotki/rotki",
  "author": "Rotki Solutions GmbH <info@rotki.com>",
  "main": "dist/main.js",
  "scripts": {
    "build": "run-p typecheck \"build:app {@}\" --",
    "build:app": "tsx scripts/build.ts",
    "build:preview": "pnpm run build:app && vite preview",
    "preview": "vite preview",
    "electron:build": "pnpm run build && pnpm run electron:package",
    "electron:package": "electron-builder build --config .electron-builder.config.ts --publish onTag",
    "electron:serve": "tsx scripts/serve.ts",
    "check:icons": "tsx scripts/extract-backend-icons.ts",
    "generate:backend-icons": "tsx scripts/extract-backend-icons.ts --generate",
    "get-protocols": "tsx scripts/get-protocols.ts",
    "link:verify": "tsx scripts/check-external-links.ts",
    "lint:style": "stylelint 'src/**/*.{vue,css}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{vue,css}'",
    "postinstall": "electron-builder install-app-deps && node scripts/postinstall.js",
    "postuninstall": "electron-builder install-app-deps",
    "serve": "tsx scripts/serve.ts --web",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:coverage": "E2E_COVERAGE=true VITE_COVERAGE=true playwright test",
    "test:e2e:coverage:report": "c8 report",
    "test:unit": "vitest --coverage",
    "test:unit:run": "vitest run --coverage",
    "typecheck": "vue-tsc --build --force",
    "traces": "vue-tsc --build --force --generateTrace traces"
  },
  "dependencies": {
    "@reown/appkit": "1.8.17",
    "@reown/appkit-adapter-ethers": "1.8.17",
    "@rotki/common": "workspace:*",
    "@rotki/ui-library": "2.12.1",
    "@types/ws": "8.18.1",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.9.0",
    "@vueuse/math": "13.9.0",
    "@vueuse/shared": "13.9.0",
    "@walletconnect/core": "2.23.4",
    "bignumber.js": "9.3.1",
    "bufferutil": "4.1.0",
    "consola": "3.4.2",
    "dayjs": "1.11.19",
    "dexie": "4.3.0",
    "echarts": "6.0.0",
    "electron-updater": "6.8.1",
    "electron-window-state": "5.0.3",
    "es-toolkit": "1.44.0",
    "ethers": "6.16.0",
    "imask": "7.6.1",
    "ofetch": "1.5.1",
    "pinia": "3.0.4",
    "ps-list": "8.1.1",
    "roboto-fontface": "*",
    "theme-colors": "0.1.0",
    "typeface-roboto-mono": "1.1.13",
    "utf-8-validate": "6.0.6",
    "vanilla-jsoneditor": "3.11.0",
    "vue": "3.5.27",
    "vue-echarts": "8.0.1",
    "vue-i18n": "11.2.8",
    "vue-router": "5.0.2",
    "ws": "8.19.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@electron/notarize": "3.1.1",
    "@intlify/unplugin-vue-i18n": "11.0.3",
    "@pinia/testing": "1.0.3",
    "@playwright/test": "1.58.0",
    "@tsconfig/node22": "22.0.5",
    "@types/node": "22.19.7",
    "@types/semver": "7.7.1",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "@vue/devtools-api": "8.0.5",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "0.7.0",
    "ajv": "8.17.1",
    "autoprefixer": "10.4.23",
    "c8": "10.1.3",
    "dmg-license": "1.0.11",
    "dotenv": "17.2.3",
    "dotenv-cli": "11.0.0",
    "electron": "39.3.0",
    "electron-builder": "26.6.0",
    "electron-store": "11.0.2",
    "fake-indexeddb": "6.2.5",
    "flush-promises": "1.0.2",
    "happy-dom": "20.4.0",
    "msw": "2.12.7",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "postcss-html": "1.8.1",
    "semver": "7.7.3",
    "source-map-support": "0.5.21",
    "stylelint": "17.0.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-order": "7.0.1",
    "tailwindcss": "3.4.19",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "unplugin-auto-import": "21.0.0",
    "unplugin-vue-components": "31.0.0",
    "vite": "7.3.1",
    "vite-plugin-checker": "0.12.0",
    "vite-plugin-vue-devtools": "8.0.5",
    "vitest": "4.0.18",
    "vue-component-type-helpers": "3.2.4",
    "vue-tsc": "3.2.4"
  }
}
